#include <iostream>

using namespace std;

int main() {
    // Input two 8-bit binary numbers
    string num1, num2;

    cout << "Enter the first 8-bit binary number: ";
    cin >> num1;

    cout << "Enter the second 8-bit binary number: ";
    cin >> num2;
    if (num1.length()!=8 || num2.length()!=8){
        cout<<"invalid"<<endl;
        return 0;
    }
    // Compare the two numbers
    if (num1 > num2) {
        cout << num1 << " is greater than " << num2 << endl;
    } else if (num1 < num2) {
        cout << num1 << " is less than " << num2 << endl;
    } else {
        cout << num1 << " is equal to " << num2 << endl;
    }

    return 0;
}
